class Main{
	static void run(){
		int a = 5; int vysl;

		if (a >= 0) {
			vysl = factorial(a);
		}else{
			
		}

		if(vysl == 120){
			ifj16.print("OK");
		}else{
			ifj16.print("NOT OK");
		}
	}

	static int factorial(int n){
		int temp_result;
		int decremented_n = n - 1;

		if (n < 2) {
			return 1;
		}else{
			temp_result = factorial(decremented_n);
			temp_result = n * temp_result;
			return temp_result;
		}
	}
}
